# BIND command
socat --experimental - SOCKS5-LISTEN:127.0.0.1:1080:fake-server:10001

socat --experimental - SOCKS5-LISTEN:127.0.0.1:1080:fake-server:8888

socat udp-listen:8888,fork,bind=127.0.0.1,fork tcp:127.0.0.1:1080


docker inspect -f '{{range .NetworkSettings.Networks}}{{.IPAddress}}{{end}}' $(docker ps -q -f "ancestor=subfuzion/netcat")
IP=`docker inspect -f '{{range .NetworkSettings.Networks}}{{.IPAddress}}{{end}}' $(docker ps -q -f "ancestor=subfuzion/netcat")`


socat --experimental - SOCKS5-LISTEN:127.0.0.1:1080:"$IP":8888

socat TCP-LISTEN:8443,fork SOCKS5:proxy_ip:remote_machine_ip:remote_port,socksport=1080
